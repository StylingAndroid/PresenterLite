<?xml version="1.0" encoding="utf-8"?>
<com.stylingandroid.presenter.engine.presentation.ConstraintSlideLayout xmlns:android="http://schemas.android.com/apk/res/android"
  xmlns:app="http://schemas.android.com/apk/res-auto"
  android:id="@id/slide"
  android:layout_width="match_parent"
  android:layout_height="match_parent"
  app:nextInAnimation="@anim/fadein"
  app:nextOutAnimation="@anim/fadeout">

  <com.stylingandroid.presenter.engine.presentation.HighlightedTextView
    android:id="@+id/title"
    style="@style/Widget.TextView.Medium"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:text="Non Zero Rule"
    app:layout_constraintBottom_toTopOf="@id/nonZero"
    app:layout_constraintEnd_toEndOf="parent"
    app:layout_constraintStart_toStartOf="parent"
    app:layout_constraintTop_toTopOf="parent"
    app:layout_constraintVertical_chainStyle="packed" />

  <com.stylingandroid.presenter.engine.presentation.AnimatedImageView
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:adjustViewBounds="true"
    android:contentDescription="@null"
    android:src="@drawable/vectors2_donut_non_zero_same"
    app:hideAnimation="@anim/fadeout"
    app:hidePhase="4"
    app:layout_constraintBottom_toBottomOf="@id/nonZero"
    app:layout_constraintEnd_toEndOf="@id/nonZero"
    app:layout_constraintStart_toStartOf="@id/nonZero"
    app:layout_constraintTop_toTopOf="@id/nonZero"
    app:showAnimation="@anim/fadein"
    app:showPhase="3" />

  <com.stylingandroid.presenter.engine.presentation.AnimatedImageView
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:adjustViewBounds="true"
    android:contentDescription="@null"
    android:src="@drawable/vectors2_donut_non_zero_different"
    app:layout_constraintBottom_toBottomOf="@id/nonZero"
    app:layout_constraintEnd_toEndOf="@id/nonZero"
    app:layout_constraintStart_toStartOf="@id/nonZero"
    app:layout_constraintTop_toTopOf="@id/nonZero"
    app:showAnimation="@anim/fadein"
    app:showPhase="8" />

  <com.stylingandroid.presenter.engine.presentation.AnimatedImageView
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:adjustViewBounds="true"
    android:contentDescription="@null"
    android:src="@drawable/vectors2_donut_non_zero_different"
    app:hidePhase="4"
    app:layout_constraintBottom_toBottomOf="@id/nonZero"
    app:layout_constraintEnd_toEndOf="@id/nonZero"
    app:layout_constraintStart_toStartOf="@id/nonZero"
    app:layout_constraintTop_toTopOf="@id/nonZero"
    app:showAnimation="@anim/fadein"
    app:showPhase="3" />

  <com.stylingandroid.presenter.engine.presentation.AnimatedImageView
    android:id="@+id/circles"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:adjustViewBounds="true"
    android:contentDescription="@null"
    android:src="@drawable/vectors2_donut_circles2"
    app:layout_constraintBottom_toBottomOf="@id/nonZero"
    app:layout_constraintEnd_toEndOf="@id/nonZero"
    app:layout_constraintStart_toStartOf="@id/nonZero"
    app:layout_constraintTop_toTopOf="@id/nonZero"
    app:synchronised="true" />

  <com.stylingandroid.presenter.engine.presentation.AnimatedImageView
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:adjustViewBounds="true"
    android:contentDescription="@null"
    android:src="@drawable/vectors2_donut_dot2a_animated"
    app:hideAnimation="@anim/none"
    app:hidePhase="1"
    app:layout_constraintBottom_toBottomOf="@id/nonZero"
    app:layout_constraintEnd_toEndOf="@id/nonZero"
    app:layout_constraintStart_toStartOf="@id/nonZero"
    app:layout_constraintTop_toTopOf="@id/nonZero"
    app:showAnimation="@anim/none"
    app:synchronised="true" />

  <com.stylingandroid.presenter.engine.presentation.AnimatedImageView
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:adjustViewBounds="true"
    android:contentDescription="@null"
    android:src="@drawable/vectors2_donut_dot2a_animated"
    app:hideAnimation="@anim/none"
    app:hidePhase="6"
    app:layout_constraintBottom_toBottomOf="@id/nonZero"
    app:layout_constraintEnd_toEndOf="@id/nonZero"
    app:layout_constraintStart_toStartOf="@id/nonZero"
    app:layout_constraintTop_toTopOf="@id/nonZero"
    app:showAnimation="@anim/none"
    app:showPhase="5"
    app:synchronised="true" />

  <com.stylingandroid.presenter.engine.presentation.AnimatedImageView
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:adjustViewBounds="true"
    android:contentDescription="@null"
    android:src="@drawable/vectors2_donut_dot2b_animated"
    app:hideAnimation="@anim/none"
    app:hidePhase="2"
    app:layout_constraintBottom_toBottomOf="@id/nonZero"
    app:layout_constraintEnd_toEndOf="@id/nonZero"
    app:layout_constraintStart_toStartOf="@id/nonZero"
    app:layout_constraintTop_toTopOf="@id/nonZero"
    app:showAnimation="@anim/none"
    app:showPhase="1"
    app:synchronised="true" />

  <com.stylingandroid.presenter.engine.presentation.AnimatedImageView
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:adjustViewBounds="true"
    android:contentDescription="@null"
    android:src="@drawable/vectors2_donut_dot2b_animated"
    app:hideAnimation="@anim/none"
    app:hidePhase="7"
    app:layout_constraintBottom_toBottomOf="@id/nonZero"
    app:layout_constraintEnd_toEndOf="@id/nonZero"
    app:layout_constraintStart_toStartOf="@id/nonZero"
    app:layout_constraintTop_toTopOf="@id/nonZero"
    app:showAnimation="@anim/none"
    app:showPhase="6"
    app:synchronised="true" />

  <com.stylingandroid.presenter.engine.presentation.AnimatedImageView
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:adjustViewBounds="true"
    android:contentDescription="@null"
    android:src="@drawable/vectors2_donut_dot2c_animated"
    app:hideAnimation="@anim/fadeout"
    app:hidePhase="3"
    app:layout_constraintBottom_toBottomOf="@id/nonZero"
    app:layout_constraintEnd_toEndOf="@id/nonZero"
    app:layout_constraintStart_toStartOf="@id/nonZero"
    app:layout_constraintTop_toTopOf="@id/nonZero"
    app:showAnimation="@anim/none"
    app:showPhase="2"
    app:synchronised="true" />

  <com.stylingandroid.presenter.engine.presentation.AnimatedImageView
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:adjustViewBounds="true"
    android:contentDescription="@null"
    android:src="@drawable/vectors2_donut_dot2c_animated"
    app:hideAnimation="@anim/fadeout"
    app:hidePhase="8"
    app:layout_constraintBottom_toBottomOf="@id/nonZero"
    app:layout_constraintEnd_toEndOf="@id/nonZero"
    app:layout_constraintStart_toStartOf="@id/nonZero"
    app:layout_constraintTop_toTopOf="@id/nonZero"
    app:showAnimation="@anim/none"
    app:showPhase="7"
    app:synchronised="true" />

  <com.stylingandroid.presenter.engine.presentation.AnimatedImageView
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:adjustViewBounds="true"
    android:contentDescription="@null"
    android:src="@drawable/vectors2_donut_dot2_grey"
    app:hideAnimation="@anim/none"
    app:hidePhase="4"
    app:layout_constraintBottom_toBottomOf="@id/nonZero"
    app:layout_constraintEnd_toEndOf="@id/nonZero"
    app:layout_constraintStart_toStartOf="@id/nonZero"
    app:layout_constraintTop_toTopOf="@id/nonZero"
    app:showAnimation="@anim/fadein"
    app:showPhase="3"
    app:synchronised="true" />

  <com.stylingandroid.presenter.engine.presentation.AnimatedImageView
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:adjustViewBounds="true"
    android:contentDescription="@null"
    android:src="@drawable/vectors2_donut_dot2_grey"
    app:hideAnimation="@anim/none"
    app:layout_constraintBottom_toBottomOf="@id/nonZero"
    app:layout_constraintEnd_toEndOf="@id/nonZero"
    app:layout_constraintStart_toStartOf="@id/nonZero"
    app:layout_constraintTop_toTopOf="@id/nonZero"
    app:showAnimation="@anim/fadein"
    app:showPhase="8"
    app:synchronised="true" />

  <com.stylingandroid.presenter.engine.presentation.AnimatedImageView
    android:id="@+id/nonZero"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:layout_marginTop="16dp"
    android:adjustViewBounds="true"
    android:contentDescription="@null"
    android:src="@drawable/vectors2_donut_arrows_same_animated"
    app:hideAnimation="@anim/fadeout"
    app:hidePhase="4"
    app:layout_constraintBottom_toBottomOf="parent"
    app:layout_constraintEnd_toEndOf="parent"
    app:layout_constraintStart_toStartOf="parent"
    app:layout_constraintTop_toBottomOf="@+id/title"
    app:synchronised="true" />

  <com.stylingandroid.presenter.engine.presentation.AnimatedImageView
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:adjustViewBounds="true"
    android:contentDescription="@null"
    android:src="@drawable/vectors2_donut_arrows_different_animated"
    app:layout_constraintBottom_toBottomOf="@id/nonZero"
    app:layout_constraintEnd_toEndOf="@id/nonZero"
    app:layout_constraintStart_toStartOf="@id/nonZero"
    app:layout_constraintTop_toTopOf="@id/nonZero"
    app:showAnimation="@anim/fadein"
    app:showPhase="4"
    app:synchronised="true" />

</com.stylingandroid.presenter.engine.presentation.ConstraintSlideLayout>
